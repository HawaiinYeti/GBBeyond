<%= tag.fieldset class: condition_fields,
                 'data-object-name': f.object_name.gsub('q', 'channel[q]') do %>

  <%= tag.legend button_to_remove_fields %>

  <%= f.attribute_fields do |a| %>
    <%= render 'attribute_fields', f: a %>
  <% end %>

  <%= f.predicate_select(compounds: false, default: :eq).gsub('"q', '"channel[q]').html_safe %>

  <%= f.value_fields do |v| %>
    <%= render 'value_fields', f: v %>
  <% end %>
<% end %>